              // Component exact match...
              <Link href="/users" :class="{ 'active': $page.component === 'Users/Index' }">Users</Link>

              // URL starts with (/users, /users/create, /users/1, etc.)...
              <Link href="/users" :class="{ 'active': $page.url.startsWith('/users') }">Users</Link>

