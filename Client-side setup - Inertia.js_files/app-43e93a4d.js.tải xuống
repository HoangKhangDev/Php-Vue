              class HandleInertiaRequests extends Middleware
              {
                  public function share(Request $request)
                  {
                      return array_merge(parent::share($request), [
                          // Synchronously...
                          'appName' => config('app.name'),

                          // Lazily...
                          'auth.user' => fn () => $request->user()
                              ? $request->user()->only('id', 'name', 'email')
                              : null,
                      ]);
                  }
              }
            `,description:'The HandleInertiaRequests middleware provides a "share" method where you can define the data that is automatically shared with each Inertia response.'}]}),m(h,{children:["Alternatively, you can manually share data using the ",i(p,{children:"Inertia::share"})," method."]}),i(L,{examples:[{name:"Laravel",language:"php",code:c`
